class Solution {
    public int[] twoSum(int[] nums, int target) {
        int sum=0;
        int res[]=new int[2];
        for(int j=0;j<nums.length;j++){
            for(int i=j+1;i<nums.length;i++){
                sum=nums[j];
                int n=target-sum;
                if(nums[i] == n){
                    res[0]=j;
                    res[1]=i;
                }
            }  
        }
        return res;
    }
}
