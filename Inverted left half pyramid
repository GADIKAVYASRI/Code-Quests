import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int r=sc.nextInt();
        int c=sc.nextInt();
        for(int i=c;i>0;i--){
            for(int j=0;j<i;j++){
                System.out.print("* ");
                }
            System.out.println(" ");
        }
    }
}
