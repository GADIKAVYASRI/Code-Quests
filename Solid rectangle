import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int r=sc.nextInt();
        int c=sc.nextInt();
        for(int i=1;i<=c;i++){
            for(int j=1;j<=r;j++){
                System.out.print("* ");
            }
            System.out.println("");
        }
    }
}
