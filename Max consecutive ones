class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        Scanner sc=new Scanner(System.in);
        int count=0,temp=0;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]==1)
            {
                count=count+1;
            }
            else
            {  
                count=0;
            }
            if(temp<count)
            {
                temp=count;
            }
        }
        return temp;
    }
}
